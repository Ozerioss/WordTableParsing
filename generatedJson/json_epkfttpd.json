{
    "$schema": "http://json-schema.org/schema#",
    "self": {
        "vendor": "laposte.fr",
        "name": "epkfttpd",
        "format": "jsonschema"
    },
    "$metadata": {
        "$datasource": "epkfttpd",
        "$dataset": "client",
        "$fileFormat": "CSV",
        "$nameFormat": "??",
        "$separator": "|",
        "$quote": "\"",
        "$escape": "\\",
        "$ingestionMode": "APPEND",
        "$dataVector": "FILE",
        "$dateFormat": "yyyy-MM-dd",
        "$dateTimeFormat": "MM-dd-yyyy HH:mm:ss"
    },
    "properties": {
        "HORODATE": {
            "description": "Timestamp",
            "type": "CHAR"
        },
        "BUKRS": {
            "description": "Société",
            "type": "CHAR"
        },
        "KAPPL": {
            "description": "Application",
            "type": "CHAR"
        },
        "KSCHL": {
            "description": "Type condition",
            "type": "CHAR"
        },
        "MWSKZ": {
            "description": "Code TVA",
            "type": "CHAR"
        },
        "DATBI": {
            "description": "Fin de validité",
            "type": "DATS"
        },
        "DATAB": {
            "description": "Début validité",
            "type": "DATS"
        },
        "KNUMH": {
            "description": "No enreg. cond.",
            "type": "CHAR"
        },
        "KOPOS": {
            "description": "No. condition",
            "type": "NUMC"
        },
        "KBETR": {
            "description": "Montant",
            "type": "CURR"
        },
        "KONWA": {
            "description": "Devise condit.",
            "type": "CUKY"
        }
    }
}